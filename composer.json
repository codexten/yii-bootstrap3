{
  "name": "codexten/yii-bootstrap3",
  "type": "yii2-extension",
  "description": "codexten bootstrap3 wrapper for yii 2.0 or higher",
  "require": {
    "yiisoft/yii2-bootstrap": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\bootstrap3\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "adminlte2": [
        "$web",
        "config/adminlte2.php"
      ]
    }
  }
}
